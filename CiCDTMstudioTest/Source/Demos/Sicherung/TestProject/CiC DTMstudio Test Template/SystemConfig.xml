<?xml version="1.0" encoding="utf-8"?>
<TestFrameworkConfiguration>
  <!-- TestConfiguration describes which 
         - Operating System Type, 
         - Frame Type, 
         - Frame Name, 
         - Frame Version, 
         - Device DTM Type (CoDIA or Classic), 
         - Device Type
         is used-->
  <TestConfiguration>
    <OperatingSystem>
      <OperatingSystemType>Win7</OperatingSystemType>
    </OperatingSystem>
    <Frame>
      <FrameType>FDT</FrameType>
      <FrameName>FieldCare</FrameName>
      <FrameVersion>V20900_1611</FrameVersion>
    </Frame>
    <CommDTM>
      <CommDTMProtocol>
      </CommDTMProtocol>
      <CommDTMName>
      </CommDTMName>
      <CommDTMVersion>
      </CommDTMVersion>
    </CommDTM>
    <DeviceDTM>
      <DeviceDTMType>CoDIA</DeviceDTMType>
      <DeviceType>Levelflex / FMP5x / HART / FW 1.02.zz / Dev.Rev. 3</DeviceType>
    </DeviceDTM>
  </TestConfiguration>
  <!-- FrameworkConfiguration provides 
         - DLL filenames and
         - parts of namespaces 
         to related implementations-->
