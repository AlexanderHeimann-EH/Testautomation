    <EnablePackageProcessLoggingAndAssert>true</EnablePackageProcessLoggingAndAssert>
  </PropertyGroup>
  
  <Import Project="$(_PythonToolsPath)\Microsoft.PythonTools.Worker.targets" />
</Project>
