                <Setter x:Uid="Setter_28" TargetName="PART_MaskedTextBox" Property="Visibility"
                        Value="Visible" />
                <Setter x:Uid="Setter_29" TargetName="Bd" Property="Visibility" Value="Hidden" />
            </Trigger>
            <Trigger x:Uid="Trigger_36" Property="UIElement.IsEnabled" Value="False">
