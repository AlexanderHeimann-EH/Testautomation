<log4net>	<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">		<layout type="log4net.Layout.PatternLayout">			<conversionPattern value="[%date] %-message %newline"/>		</layout>	</appender>	<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">		<file value=".\C4DC.log"/>		<appendToFile value="true"/>		<maximumFileSize value="100KB"/>		<maxSizeRollBackups value="500"/>		<layout type="log4net.Layout.PatternLayout">			<conversionPattern value="[%date] %-message %newline"/>		</layout>	</appender>	<root>		<level value="All"/>		<appender-ref ref="ConsoleAppender"/>		<appender-ref ref="RollingFile"/>	</root></log4net>