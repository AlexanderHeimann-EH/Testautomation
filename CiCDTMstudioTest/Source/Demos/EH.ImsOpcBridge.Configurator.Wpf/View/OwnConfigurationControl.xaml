<UserControl x:Uid="UserControl_1" x:Class="EH.ImsOpcBridge.Configurator.View.OwnConfigurationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:viewModel="clr-namespace:EH.ImsOpcBridge.Configurator.ViewModel"
             xmlns:view="clr-namespace:EH.ImsOpcBridge.Configurator.View"
             d:DataContext="{d:DesignInstance {x:Type viewModel:OwnConfigurationControlVm}, IsDesignTimeCreatable=True}"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="600">

    <Grid  x:Uid="Grid_1">
          <ContentControl x:Uid="ContentControl1"  x:Name="ContentControl1" Focusable="False" Content="{Binding Content}" />
    </Grid>
    
</UserControl>             
             
