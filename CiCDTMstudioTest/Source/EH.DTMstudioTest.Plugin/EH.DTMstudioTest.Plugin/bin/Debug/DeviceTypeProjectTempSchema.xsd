<?xml version="1.0" encoding="Windows-1252"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:schemaLocation="PCPS/TestData TempCoDIADTMstudioTestData.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
  <xsd:element name="DTMstudioCoDIAData">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceTypeProject">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="Guid" type="xsd:string" />
              <xsd:element name="FDTDeviceTypeName" type="xsd:string" />
              <xsd:element name="DeviceFunctions">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" name="DeviceFunction">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element name="Name" type="xsd:string" />
                          <xsd:element name="CompilerVariable" type="xsd:string" />
                          <xsd:element name="DisplayName" type="xsd:string" />
                          <xsd:element name="Active" type="xsd:boolean" />
                          <xsd:element name="VersionString" type="xsd:string" />
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="DeviceTypeFramework">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="FrameworkComponents">
                      <xsd:complexType>
                        <xsd:sequence>
                          <xsd:element maxOccurs="unbounded" name="FrameworkComponent">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="Name" type="xsd:string" />
                                <xsd:element name="DisplayName" type="xsd:string" />
                                <xsd:element name="VersionString" type="xsd:string" />
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="VersionString" type="xsd:string" />
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xs:schema>